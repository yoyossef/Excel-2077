{
  "tags": {
    "allowUnknownTags": [
      "category"
    ]
  },
  "source": {
    "recurseDepth": 10,
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "plugins": [
    "node_modules/better-docs/component",
    "node_modules/better-docs/category"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "template": "node_modules/better-docs"
  }
}